	function temp = convertTempFor(InVec,str)
    if strcmp(str,'C2F')
        temp = zeros(size(InVec));
        for counter = 1:length(InVec)
            temp(counter) = (InVec(counter)*(9/5)) + 32;
        end
    elseif strcmp(str,'F2C')
        temp = zeros(size(InVec));
        for counter = 1:length(InVec)
            temp(counter) = (InVec(counter) -32)*(5/9);
        end
    else
        disp('Error!')
    end
end

	>> convertTempFor(InVec,'F2C')

