   function temp = convertTempWhile(InVec, str)
    counter = 1;
    temp_val = size(InVec);
    if strcmp(str,'F2C')
        temp = zeros(size(InVec));
        while temp_val(2) >= counter
            temp(counter) = (InVec(counter) - 32)*(5/9);
            counter = counter + 1;
        end
    elseif strcmp(str,'C2F')
        temp = zeros(size(InVec));
        while temp_val(2) >= counter
            temp(counter) = (InVec(counter)*(9/5)+32);
            counter = counter + 1;
        end
    else
        disp('Error!')
    end
end

>> convertTempWhile(InVec,'C2F')

