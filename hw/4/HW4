function temp = convertTempVec(InVec,str)
    if strcmp(str, 'C2F')
        temp = InVec.*(9.0/5.0) + 32.0;
    elseif strcmp(str,'F2C')
        temp = (InVec-32.0)./1.8;
    else
        disp('Error!')
    end
end

